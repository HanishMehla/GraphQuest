{
  "env": {
    "es2022": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "error",
    "no-undef": "error",
    "no-console": "off",
    "semi": ["error", "always"],
    "eqeqeq": ["error", "always"]
  }
}